import java.util.Scanner;
class first{
String name;
String div;
String branch;
String prn;
int roll;
float cgpa;

public void input()
{
Scanner myObj=new Scanner(System.in);

System.out.print("Enter the student name:");
name=myObj.nextLine();

System.out.print("Enter the student Div:");
div=myObj.nextLine();

System.out.print("Enter the student branch:");
branch=myObj.nextLine();

System.out.print("Enter the student PRN No:");
prn=myObj.nextLine();

System.out.print("Enter the student Roll no:");
roll=myObj.nextInt();
if(roll<0)
{
do
{
System.out.println("Roll Should not be negative,Try again!!");
System.out.print("Enter the student Roll no:");
roll=myObj.nextInt();
}while(roll<0);
}
System.out.print("Enter the up to 4 semister[cgpa]: ");
cgpa=myObj.nextFloat();
}


 public void display()
{
System.out.println("Name: "+name);
System.out.println("div: "+div);
System.out.println("branch: "+branch);
System.out.println("roll no: "+roll);
System.out.println("PRN no: "+prn);
System.out.println("CGPA: "+cgpa);
if(cgpa>9)
{
System.out.print("performace: Excellent");
}
else if(cgpa>7.5)
{
System.out.print("performace: Good");
}
else
{
System.out.print("performace: Average");
}
}
}
 class Sy{
public static void main(String[] args)
{
first s[]=new first[10];
Scanner myObj1=new Scanner(System.in);
int ch,n=0,i;
do
{
System.out.println("1.insert data "+" 2.display data ");
System.out.print(" Enter Your choice:");
ch=myObj1.nextInt();
switch(ch)
{
case 1:
System.out.println("No. of student you want to insert:");
n=myObj1.nextInt();
for(i=0;i<n;i++){
s[i]=new first();
System.out.println(i+1+"]");
s[i].input();
}
break;

case 2:
for( i=0;i<n;i++){
System.out.println("\n"+i+1+"]");
s[i].display();
}
break;

default:
System.out.println("Wrong choice!!");
}
System.out.println("\nDo you want to continue[press 1 for continue]");
ch=myObj1.nextInt();
}while(ch==1);
}
}
